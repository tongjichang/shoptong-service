<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="com.haffee.heygay.po">
    <class name="ShopPrinter">
        <comment>打印机表</comment>
        <id name="id" type="integer">
            <column name="ID" length="10">
            	<comment>主键</comment>
            </column>
            <generator class="identity"></generator>
        </id>
        <property name="type_print" type="string">
        	<column name="TYPE_PRINT" length="11">
        		<comment>打印类型</comment>
        	</column>
        </property>
        <property name="shop_id" type="integer">
        	<column name="SHOP_ID" length="10">
        		<comment>所属店铺ID</comment>
        	</column>
        </property>
        <property name="printer_no" type="string">
        	<column name="PRINTER_NO" length="50">
        		<comment>打印机编号</comment>
        	</column>
        </property>
        <property name="printer_name" type="string">
        	<column name="PRINTER_NAME" length="50">
        		<comment>打印机名称</comment>
        	</column>
        </property>
        <property name="create_time" type="timestamp">
        	<column name="CREATE_TIME" length="20">
        		<comment>创建时间</comment>
        	</column>
        </property>  
        
    </class>
</hibernate-mapping>
